{
    "name": "litegraph.tsx",
    "version": "0.6.2",
    "description": "A flow graph node editor similar to LiteGraph.js, Blender Nodes, or ReactFlow.  It works in an HTML5 Canvas and allows to export graphs to be included in applications.",
    "main": "build/litegraph.core.js",
    "type": "module",
    "types": "src/litegraph.d.ts",
    "private": false,
    "scripts": {
        "build": "npx tsc -p ./tools/tsconfig.json && vite build",
        "doc": "jsdoc -c ./tools/jsdoc.config.json -d ./documentation -r --verbose --debug",
        "dev": "vite dev",
        "preview": "vite preview",
        "test": "vitest",
        "lint": "npx eslint --cache src",
        "lint:fix": "npx eslint --fix src",
        "madge:png": "npx madge --image assets/dev/dep_graph.png --ts-config ./tools/tsconfig.json src",
        "typecheck": "tsc -p ./tools/tsconfig.strict.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/litegraph-tsx/litegraph.tsx.git"
    },
    "author": "",
    "contributors": [
        {
            "name": "Javi",
            "url": "http://github.com/jagenjo"
        },
        {
            "name": "Daniel Lewis",
            "url": "http://github.com/daniel-lewis-ab"
        },
        {
            "name": "Atlasan",
            "url": "http://github.com/atlasan"
        },
        {
            "name": "DrJKL",
            "url": "http://github.com/DrJKL"
        }
    ],
    "license": "MIT",
    "files": [
        "build",
        "css/litegraph.css",
        "src/litegraph.d.ts"
    ],
    "bugs": {
        "url": "https://github.com/litegraph-tsx/litegraph.tsx/issues"
    },
    "homepage": "https://github.com/litegraph-tsx/litegraph.tsx#readme",
    "devDependencies": {
        "@eslint/eslintrc": "^3.1.0",
        "@eslint/js": "^9.7.0",
        "@stylistic/eslint-plugin": "^2.3.0",
        "@stylistic/eslint-plugin-js": "^2.3.0",
        "@types/eslint__js": "^8.42.3",
        "@typescript-eslint/parser": "^7.16.1",
        "eslint": "^8.57.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-import-resolver-node": "^0.3.9",
        "eslint-import-resolver-typescript": "^3.6.1",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^27.9.0",
        "jsdoc": "^4.0.3",
        "madge": "^7.0.0",
        "pnpm": "^9.5.0",
        "typescript": "^5.5.3",
        "vite": "^5.3.3",
        "vitest": "^2.0.3"
    }
}
